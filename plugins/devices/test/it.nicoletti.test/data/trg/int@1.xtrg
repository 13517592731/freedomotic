<trigger>
  <name>It&apos;s morning</name>
  <description>it is morning (it starts at 8:00)</description>
  <channel>app.event.sensor.calendar.event.schedule</channel>
  <payload>
    <payload>
      <it.freedomotic.reactions.Statement>
        <logical>AND</logical>
        <attribute>time.hour</attribute>
        <operand>GREATER_THEN</operand>
        <value>7</value>
      </it.freedomotic.reactions.Statement>
      <it.freedomotic.reactions.Statement>
        <logical>AND</logical>
        <attribute>time.hour</attribute>
        <operand>LESS_THEN</operand>
        <value>13</value>
      </it.freedomotic.reactions.Statement>
    </payload>
  </payload>
  <hardwareLevel>false</hardwareLevel>
  <delay>0</delay>
  <priority>0</priority>
  <maxExecutions>0</maxExecutions>
  <numberOfExecutions>0</numberOfExecutions>
  <suspensionTime>39600000</suspensionTime>
</trigger>